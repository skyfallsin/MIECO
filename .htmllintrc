    "tag-bans": ["style", "b", "i"],
    "tag-close": true,
    "tag-name-lowercase": true,
    "tag-name-match": true,
    "tag-req-attr": false,
    "tag-self-close": false,
    "text-ignore-regex": false,
    "title-max-len": 60,
    "title-no-dup": true
